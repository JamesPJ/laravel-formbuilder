{
    "name": "jamespj/form-builder",
    "description": "Laravel Package for Creating a Drag-and-Drop Form Builder Using JQuery Form Builder",
    "authors": [
        {
            "name": "James PJ",
            "email": "hello@jamespj.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": ">=5.3",
        "illuminate/support": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "Jamespj\\FormBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jamespj\\FormBuilder\\FormBuilderServiceProvider"
            ]
        }
    }
}
